def find_minimum(numbers):
    if not numbers:
        return None    
    min_value = numbers[0] 
    for number in numbers:
        if number < min_value:
            min_value = number 
    return min_value
numbers = [3, 1, 4, 1, 5, 9, 2, 6, 5, 3, 5]
print(f"The minimum value in the list is: {find_minimum(numbers)}")
